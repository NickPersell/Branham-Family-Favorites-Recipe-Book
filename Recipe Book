<div class="recipe">
    <h2 onclick="toggleRecipe(this)">Zucchini Relish - <span class="author">Bev Branham</span></h2>
    <div class="recipe-content">
        <div class="ingredients"><strong>Ingredients:</strong><ul>
            <li>10 c. chopped or grated zucchini</li>
            <li>4 c. green pepper (chopped)</li>
            <li>2 onions (chopped)</li>
            <li>5 T. salt</li>
            <li>2½ c. vinegar</li>
            <li>6 c. sugar</li>
        </ul></div>
        <div class="ingredients"><strong>Ingredients:</strong><ul>
            <li>2 tsp. celery seed</li>
            <li>2 tsp. black pepper</li>
            <li>2 T. flour (heaping)</li>
            <li>1 tsp. dry mustard</li>
            <li>2 tsp. turmeric</li>
            <li>2 tsp. nutmeg</li>
        </ul></div>
        <p><strong>Instructions:</strong> Combine zucchini, green pepper, onions and salt. Let set in refrigerator overnight. Drain off 1 cup of liquid and throw away. Mix rest of ingredients into zucchini mixture. Cook on simmer about 30 minutes. Put into hot jars and seal. This makes 7 pints.</p>
    </div>
</div>
    <!-- End of recipes -->
    <script>
        // Toggle recipe content visibility
        function toggleRecipe(element) {
            const content = element.nextElementSibling;
            content.classList.toggle('show');
        }

        // Search functionality
        function searchRecipes() {
            const input = document.getElementById('searchInput').value.toLowerCase();
            const recipes = document.getElementsByClassName('recipe');
            
            for (let i = 0; i < recipes.length; i++) {
                const recipeText = recipes[i].textContent.toLowerCase();
                recipes[i].classList.remove('hidden'); // Reset hidden state
                if (input !== '' && !recipeText.includes(input)) {
                    recipes[i].classList.add('hidden');
                }
            }
            updateExpandCollapse(); // Update expand/collapse state
            // Removed applyCategoryFilter() to prevent overriding search results
        }

        // Expand all visible recipes
        function expandAll() {
            const visibleRecipes = document.querySelectorAll('.recipe:not(.hidden)');
            visibleRecipes.forEach(recipe => {
                const content = recipe.querySelector('.recipe-content');
                content.classList.add('show');
            });
        }

        // Collapse all visible recipes
        function collapseAll() {
            const visibleRecipes = document.querySelectorAll('.recipe:not(.hidden)');
            visibleRecipes.forEach(recipe => {
                const content = recipe.querySelector('.recipe-content');
                content.classList.remove('show');
            });
        }

        // Update expand/collapse state after search or category filter
        function updateExpandCollapse() {
            // No additional action needed as functions handle visibility
        }

        // Define category ranges
        const categories = [
            { min: 1, max: 37, label: "Appetizers & Beverages" },
            { min: 38, max: 119, label: "Soups & Salads" },
            { min: 120, max: 156, label: "Vegetables & Sides Dishes" },
            { min: 157, max: 249, label: "Main Dishes" },
            { min: 250, max: 282, label: "Breads & Rolls" },
            { min: 283, max: 396, label: "Desserts" },
            { min: 397, max: 451, label: "Cookies & Candy" },
            { min: 452, max: 500, label: "This and That" }
        ];

        // Create category buttons
        function createCategoryButtons() {
            const categoryContainer = document.getElementById('categoryButtons');
            categories.forEach(category => {
                const button = document.createElement('button');
                button.textContent = category.label;
                button.addEventListener('click', () => filterByCategory(category.min, category.max));
                categoryContainer.appendChild(button);
            });
            // Add a "Show All" button
            const allButton = document.createElement('button');
            allButton.textContent = "Show All";
            allButton.addEventListener('click', () => showAllRecipes());
            categoryContainer.appendChild(allButton);
        }

        // Assign entry numbers dynamically
        function assignEntryNumbers() {
            const recipes = document.getElementsByClassName('recipe');
            for (let i = 0; i < recipes.length; i++) {
                recipes[i].setAttribute('data-entry', i + 1); // Assign 1-based entry numbers
            }
        }

        // Filter recipes by category range
        function filterByCategory(min, max) {
            const recipes = document.getElementsByClassName('recipe');
            for (let i = 0; i < recipes.length; i++) {
                const entry = parseInt(recipes[i].getAttribute('data-entry'));
                if (entry >= min && entry <= max) {
                    recipes[i].classList.remove('hidden');
                } else {
                    recipes[i].classList.add('hidden');
                }
            }
            updateExpandCollapse();
        }

        // Show all recipes
        function showAllRecipes() {
            const recipes = document.getElementsByClassName('recipe');
            for (let i = 0; i < recipes.length; i++) {
                recipes[i].classList.remove('hidden');
            }
            updateExpandCollapse();
        }

        // Apply category filter after search to maintain consistency
        function applyCategoryFilter() {
            const activeCategory = categories.find(cat => {
                const recipes = document.querySelectorAll(`.recipe:not(.hidden)`);
                return Array.from(recipes).every(recipe => {
                    const entry = parseInt(recipe.getAttribute('data-entry'));
                    return entry >= cat.min && entry <= cat.max;
                }) && recipes.length > 0;
            }) || { min: 1, max: 500 };
            filterByCategory(activeCategory.min, activeCategory.max);
        }

        // Initial setup
        window.onload = function() {
            assignEntryNumbers();
            createCategoryButtons();
            searchRecipes();
        };
    </script>
</body>
</html>
